/*
	Greater Header Dependencies:

	misc →┬─────────────────────────→┬→ u8c
	      ├→ arr ─→┬────────────────→┤
	      │        └→ utf ─→┬───────→┤
	      │                 └→ str ─→┤
	      └→ cstr →┬────────────────→┤
	               └→ impl →┬───────→┤
	                        └→ math →┘
*/

#if !defined(u8c_key_piDyeERQmK9By1n3)
#define u8c_key_piDyeERQmK9By1n3

#include <concepts>    /* std::convertible_to */
#include <ostream>     /* std::ostream */
#include <type_traits> /* std::is_same_v */

#include <u8c/math>
#include <u8c/str>

namespace u8c {
	[[nodiscard]] constexpr auto uniblk(char32_t chr) -> u8c::str;
	[[nodiscard]] constexpr auto uninm( char32_t chr) -> u8c::str;
}

#include <u8c/u8c.d/uniblk>
#include <u8c/u8c.d/uninm>

#endif
